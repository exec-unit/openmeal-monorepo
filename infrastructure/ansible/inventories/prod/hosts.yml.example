---
all:
  children:
    openmeal_prod:
      hosts:
        prod-server:
          ansible_host: 192.168.1.102
          ansible_user: admin
          ansible_port: 22
          ansible_ssh_private_key_file: ~/.ssh/id_rsa
          deployment_type: prod
          ansible_python_interpreter: /usr/bin/python3

      vars:
        ansible_become: yes
        ansible_become_method: sudo
