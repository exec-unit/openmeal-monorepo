---
all:
  children:
    openmeal_shared_dev:
      hosts:
        shared-dev-server:
          ansible_host: 192.168.1.101
          ansible_user: admin
          ansible_port: 22
          ansible_ssh_private_key_file: ~/.ssh/id_rsa
          deployment_type: shared-dev
          ansible_python_interpreter: /usr/bin/python3

      vars:
        ansible_become: yes
        ansible_become_method: sudo
